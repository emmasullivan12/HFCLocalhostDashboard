    },
    {
        "type": "message",
        "subtype": "bot_message",
        "text": "I'm happy to answer any questions you have, and give you a hand.",
        "ts": "1552307790.001900",
        "username": "Dave Edwardz (Prospela Mentors (beta))",
        "icons": {
