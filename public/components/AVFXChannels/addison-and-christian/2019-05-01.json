        "username": "Addison Petrie (Prospela Mentors (beta))",
        "icons": {
            "image_48": "https:\/\/s3-us-west-2.amazonaws.com\/slack-files2\/bot_icons\/2019-05-01\/612241477906_48.png"
        },
        "bot_id": "B6CS60LDU"
