    {
        "type": "message",
        "text": "I'm currently an operational manager at the Royal Free London Hospitals so I cover three main hospital sites, next week however I'm moving into a more strategic role",
        "user": "U5WR9CJTC",
        "ts": "1499148820.583471",
        "team": "T4UUTPV18",
        "user_team": "T4UUTPV18",
        "source_team": "T4UUTPV18",
        "user_profile": {
            "avatar_hash": "12cc30392690",
