            "image_72": "https:\/\/secure.gravatar.com\/avatar\/913115cfbdb31de8ad2ceb1b7fc76646.jpg?s=72&d=https%3A%2F%2Fa.slack-edge.com%2Fdf10d%2Fimg%2Favatars%2Fava_0015-72.png",
