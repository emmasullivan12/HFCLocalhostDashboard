    {
        "type": "message",
        "text": "i work for a consultancy that provides expert witness services",
        "user": "U5WMH4QGK",
        "ts": "1498824334.171346",
        "team": "T4UUTPV18",
        "user_team": "T4UUTPV18",
        "source_team": "T4UUTPV18",
        "user_profile": {
